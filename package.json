        "@angular/material": "^15.1.2",
        "@angular/platform-browser": "^15.1.3",
        "@angular/platform-browser-dynamic": "^15.1.3",
        "@angular/router": "^15.1.3",
        "@ngneat/until-destroy": "^9.2.3",
        "@ngx-translate/core": "^14.0.0",
        "@ngx-translate/http-loader": "^7.0.0",
